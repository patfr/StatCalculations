{
    "compilerOptions": {
      "target": "ESNext",
      "lib": ["ESNext", "DOM"],
      "module": "ESNext",
      "outDir": "public/js",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "strict": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noUncheckedIndexedAccess": true,
      "skipLibCheck": false,
      "allowJs": true
    },
    "exclude": [
      "public",
    ]
  }
  